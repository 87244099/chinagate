<!--pages/productSearch/productSearch.wxml-->
<import src="../../templates/productSearch/productSearch.wxml"/>
<view class="fk_page">
  <view class="fk_productSearch">
    <mp-searchbar class="fk_productSearch_searchBar" placeholder="搜索产品" bindselectresult="selectResult" value="{{setting.inputWord}}" bindblur="searchBlur" bindclear="searchClear" cancel="{{false}}"></mp-searchbar>
    <view class="fk_productSearch_word">当前搜索词：{{setting.word}}</view>
    <view class="fk_productSearch_main">
      <view class="fk_productSearch_product_list">
        <product class="fk_productSearch_product_item" wx:key="productID" wx:for="{{setting.productList}}" config="{{config}}" product="{{item}}" setting="{{setting}}"></product>
      </view>
    </view>
  </view>
  <tab-bar class="fk_tabBar" type="company" rollbackVisible="{{_commPageConfigData.rollbackVisible}}" companyInfo="{{pageData.companyPageData.companyInfo}}"></tab-bar>
  <van-toast id="van-toast"/>
</view>
