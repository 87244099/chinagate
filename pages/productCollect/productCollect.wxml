<!--pages/productCollect/productCollect.wxml-->
<view class="fk_page">
  <view class="fk_productCollect">
    <view class="fk_productCollect_list">
      <navigator class="fk_productSearch_product_item" url="/pages/productDetail/productDetail?id={{item.productID}}" wx:key="productID" wx:for="{{setting.productList}}">
        <view class="fk_productSearch_product_img">
          <image style="width:100%;height:100%" mode="" src="{{staticDomain}}/{{item.smallImgPath}}"></image>
        </view>
        <view class="fk_productSearch_product_name">{{item.title}}</view>
        <view class="fk_productSearch_product_priceInfo">
          <view class="fk_productSearch_product_priceInfo_line">
            <view class="fk_productSearch_product_priceInfo_label">普通批发价：</view>
            <view class="fk_productSearch_product_priceInfo_value">868286</view>
          </view>
          <view class="fk_productSearch_product_priceInfo_line">
            <view class="fk_productSearch_product_priceInfo_label">vip批发价：</view>
            <view class="fk_productSearch_product_priceInfo_value">868286</view>
          </view>
        </view>
      </navigator>
    </view>
  </view>
</view>