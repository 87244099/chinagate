<view>
  <view class="fk_shop_list">
    <navigator class="fk_shops_item"  data-item="{{item}}" catchtap="onJumpCard">
      <view class="fk_shops_item_left">
        <view class="fk_shops_item_img">
          <image style="width:100%;height:100%;" mode="aspectFill" src="{{config.domain}}/{{item.avatarPhoto}}"></image>
        </view>
        <view class="fk_shops_item_info">
          <view class="fk_shops_item_info_name">名片收藏：{{item.name}}</view>
        </view>
      </view>
      <view class="fk_shops_item_right">
        <view class="fk_shops_item_right_icon"></view>
      </view>
    </navigator>
  </view>
  <tab-bar class="fk_tabBar"></tab-bar>
  <van-dialog id="van-dialog" />
</view>